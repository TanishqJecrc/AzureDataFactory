{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datapipeline213"
		},
		"ADLS Taxi_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS Taxi'"
		},
		"BlobStorageProducts_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageProducts'"
		},
		"ProductDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ProductDb'"
		},
		"Taxidb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Taxidb'"
		},
		"AdventureWorks_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdventureWorks'"
		},
		"ADLS Taxi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azureadlspipeline.dfs.core.windows.net/"
		},
		"ProductDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlservertanishq.database.windows.net"
		},
		"ProductDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqlservertanishq"
		},
		"ProductDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "tanishq"
		},
		"Taxidb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlservertanishq.database.windows.net"
		},
		"Taxidb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "taxi-data321"
		},
		"Taxidb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "tanishq"
		},
		"AdventureWorks_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlservertanishq.database.windows.net"
		},
		"AdventureWorks_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksDW2022"
		},
		"AdventureWorks_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "tanishq"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datapipeline213_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS Taxi_accountKey": {
						"value": "[parameters('ADLS Taxi_accountKey')]"
					},
					"BlobStorageProducts_connectionString": {
						"value": "[parameters('BlobStorageProducts_connectionString')]"
					},
					"ProductDb_password": {
						"value": "[parameters('ProductDb_password')]"
					},
					"Taxidb_password": {
						"value": "[parameters('Taxidb_password')]"
					},
					"AdventureWorks_password": {
						"value": "[parameters('AdventureWorks_password')]"
					},
					"ADLS Taxi_properties_typeProperties_url": {
						"value": "[parameters('ADLS Taxi_properties_typeProperties_url')]"
					},
					"ProductDb_properties_typeProperties_server": {
						"value": "[parameters('ProductDb_properties_typeProperties_server')]"
					},
					"ProductDb_properties_typeProperties_database": {
						"value": "[parameters('ProductDb_properties_typeProperties_database')]"
					},
					"ProductDb_properties_typeProperties_userName": {
						"value": "[parameters('ProductDb_properties_typeProperties_userName')]"
					},
					"Taxidb_properties_typeProperties_server": {
						"value": "[parameters('Taxidb_properties_typeProperties_server')]"
					},
					"Taxidb_properties_typeProperties_database": {
						"value": "[parameters('Taxidb_properties_typeProperties_database')]"
					},
					"Taxidb_properties_typeProperties_userName": {
						"value": "[parameters('Taxidb_properties_typeProperties_userName')]"
					},
					"AdventureWorks_properties_typeProperties_server": {
						"value": "[parameters('AdventureWorks_properties_typeProperties_server')]"
					},
					"AdventureWorks_properties_typeProperties_database": {
						"value": "[parameters('AdventureWorks_properties_typeProperties_database')]"
					},
					"AdventureWorks_properties_typeProperties_userName": {
						"value": "[parameters('AdventureWorks_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datapipeline213_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datapipeline213_ArmTemplate_0')]"
			]
		},
		{
			"name": "datapipeline213_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datapipeline213_ArmTemplate_1')]"
			]
		}
	]
}