{
	"name": "Json Arm Pipline Parameterized",
	"properties": {
		"activities": [
			{
				"name": "Data flow",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "DataFlow",
						"type": "DataFlowReference",
						"datasetParameters": {
							"source1": {
								"sourcePath": {
									"value": "@pipeline().parameters.sourcePath",
									"type": "Expression"
								}
							},
							"sink1": {
								"fileType": {
									"value": "@pipeline().parameters.fileType",
									"type": "Expression"
								},
								"sinkPath": {
									"value": "@pipeline().parameters.sinkPath",
									"type": "Expression"
								}
							}
						}
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"parameters": {
			"sourcePath": {
				"type": "string",
				"defaultValue": "raw-Zone"
			},
			"sinkPath": {
				"type": "string",
				"defaultValue": "raw-Zone"
			},
			"fileType": {
				"type": "string",
				"defaultValue": "csv"
			}
		},
		"annotations": []
	}
}